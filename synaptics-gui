    Callback = function(Value)
        
    end})

--notification
Rayfield:Notify({Title = "Loaded",Content = "The script has loaded succesfully.,",Duration = 6.5,Image = 4483362458,Actions = { Ignore = {Name = "Okay!",Callback = function()end},},})
while wait(1) do
    PingInfo:Set("You have ".. getfps().. " FPS and ".. getping().. " ping")
    UserInfo:Set("Your username is ".. getplayername().. " (User ID: "..getplayerid().. ")")
    GameInfo:Set("You're playing ".. getgamename().. " (Game ID: ".. getgameid().. ")")
    ExecutorInfo:Set("You're using ".. identifyexecutor().. " (Level ".. getthreadidentity().. ")")
end
