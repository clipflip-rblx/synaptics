--commands
    local EmptySectionCmds = Cmds:CreateSection("")
    local Noclip = Cmds:CreateToggle({
   Name = "Noclip",CurrentValue = false,Callback = function(Value)
    if Value == true then
        local speaker = game.Players.LocalPlayer
        for _, child in pairs(speaker.Character:GetDescendants()) do
            if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= floatName then
                child.CanCollide = false
            end
        end
    else
        local speaker = game.Players.LocalPlayer
        for _, child in pairs(speaker.Character:GetDescendants()) do
            if child:IsA("BasePart") and child.CanCollide == false and child.Name ~= floatName then
                child.CanCollide = true
            end
        end
    end
   end,})
       local fly = Cmds:CreateButton({Name = "Fly",Callback = function()
    loadstring(game:HttpGet('https://pastefy.app/nFLFeUWS/raw'))()
   end,})
   local walkspeed = Cmds:CreateSlider({Name = "Set your Walkspeed",
   Range = {0, 1000},Increment = 1,
   Suffix = "Walkspeed",
   CurrentValue = walkspeed,
   Callback = function(Value)
    human.WalkSpeed = Value
   end,
})
   local jumppower = Cmds:CreateSlider({Name = "Set your Jumppower",
   Range = {0, 1000},Increment = 1,
   Suffix = "Jumppower",
   CurrentValue = jumppower,
   Callback = function(Value)
    human.JumpPower = Value
   end,
})
       local infjump = Cmds:CreateButton({Name = "Infinite Jump",Callback = function()
        game:GetService("UserInputService").JumpRequest:connect(function()
            human:ChangeState("Jumping")
        end)
   end,})

