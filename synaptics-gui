local EmptySectionMisc = Misc:CreateSection("")
local AxBanHWID = Misc:CreateButton({Name = "Arceus X Ban Checker",Callback = function()
        local http = cloneref(game:GetService("HttpService"));local succ, result = pcall(function()return http:JSONDecode(game:HttpGet("https://raw.githubusercontent.com/SPDM-Team/Arceus-X-NEO-public/main/bannedusers"))
        end)
        if succ and result[gethwid()] then Rayfield:Notify({Title = "Ban Check Result",Content = "You are banned from Arceus X.",Duration = 6.5,Image = 4483362458,Actions = { Ignore = {Name = "Okay!",Callback = function()end},},}) else Rayfield:Notify({Title = "Ban Check Result",Content = "You are not banned from Arceus X.",Duration = 6.5,Image = 4483362458,Actions = { Ignore = {Name = "Okay!",Callback = function()end},},})
        end
   end,})
local Kick = Misc:CreateInput({
   Name = "Kick Yourself",
   PlaceholderText = "Reason",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
    kick(Text)
   end,
})
local Execute = Misc:CreateInput({
   Name = "Execute",
   PlaceholderText = "Code",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
   if Text == "print(identifyexecutor())" then
       print("Synaptics ".. Synapticsver)
    elseif Text == "print(getexecutorname())" then
        print("Synaptics ".. Synapticsver)
    elseif Text == "print(getexecutorversion())" then
        print(Synapticsver)
    else
   loadstring(Text)()
    end
   end,
})

